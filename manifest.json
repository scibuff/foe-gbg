{
  "name": "FoE GBg",
  "version": "1.0.0.0",
  "minimum_chrome_version": "10.0",
  "description": "FoE GBG uses the Guild Battleground participants leaderboard to track guild members negotiations and fights",
  "devtools_page": "devtools.html",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "content_security_policy": "script-src 'self' https://code.jquery.com; object-src 'self'",
  "manifest_version": 2
}